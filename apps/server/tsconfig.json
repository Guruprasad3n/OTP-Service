{
		"compilerOptions": {
			"target": "es6",
			"module": "commonjs",
			"esModuleInterop": true,
			"skipLibCheck": true,
			"forceConsistentCasingInFileNames": true,
			"strict": true,
			"moduleResolution": "node",
			"resolveJsonModule": true,
			"isolatedModules": true,
			"noEmit": true,
			"jsx": "react",
			"types": ["node", "jest"]
		  },
		  "include": ["src/**/*.ts"],
		  "exclude": ["node_modules"]
}